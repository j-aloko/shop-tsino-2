#!/usr/bin/env sh
. "$(dirname -- "$0")/_/husky.sh"


# Check ESLint Standards
npm run lint ||
(
        echo '游땫游游녦游땫 ESLint Check Failed. Make the required changes listed above, add changes and try to commit again. 游땫游游녦游땫
                '
        false;
)

# If everything passes... Now we can commit
echo '九九九九 You win this time... I am committing this now. 九九九九'
